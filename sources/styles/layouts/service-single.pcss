section#hero {
  background: no-repeat center right / cover
    url('../../images/service-single-hero-bg.jpg');

  div {
    highlighted-text.yellow {
      --text: 'Service sINGLE';
    }
    h1 {
      --text: 'Services Details';
    }
  }
}
section#financial-projections {
  display: grid;
  gap: clamp(30px, pxToVw(70), 70px);
  background: var(--gray);
  padding: var(--content-block-padding) var(--content-inline-padding);

  three-section-block {
    header-section {
      img {
        --attr-src: './images/icons/stat_desc.svg';
        --attrs: "loading='lazy' alt=''";
      }

      h2 {
        --text: 'Financial Projections';
      }
      p {
        --text: 'Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.';
      }
    }
    buttons-block {
      button.advantages-prev {
        --attr-type: 'button';
        --attr-title: 'prev';

        img {
          --attr-src: './images/icons/toLeft.svg';
          --attrs: "loading='lazy' alt=''";
        }
      }
      button.advantages-next {
        --attr-type: 'button';
        --attr-title: 'next';

        img {
          --attr-src: './images/icons/yellowToRight.svg';
          --attrs: "loading='lazy' alt=''";
        }
      }
    }
  }
