section#vision {
  padding-bottom: 50px;

  images-block.decorated-image-small {
    x-img {
      --attr-source: './images/exept-decor.jpg';
      --attrs: "loading='lazy' alt='' webp avif";
    }
  }
  text-block {
    header-section {
      highlighted-text.yellow {
        --text: "WE'LL WORK WITH YOU";
      }
      h2 {
        --text: 'What To Expect';
      }
      p {
        --text: 'Capitalize on low hanging fruit to identify a ballpark value added activity beta test. Override the digital divide with additional from DevOps.';
      }
    }

    ul {
      li {
        p {
          --text: 'Bring to the table win-win survival strategies to ensure proactive domination. At the end of the day.';
        }
      }
      li {
        p {
          --text: 'Capitalize on low hanging fruit to identify a ballpark value added activity to beta test override the digital.';
        }
      }
    }
  }
}
infinite-scroll#clients-line {
  --duration: 30s @426 20s;
  --gap: clamp(40px, pxToVw(130), 130px);

  padding-block: 50px;
  align-content: center;

  img {
    --attrs: 'src="./images/clients/1.svg" alt=""';
  }

  img {
    --attrs: 'src="./images/clients/2.svg" alt=""';
  }

  img {
    --attrs: 'src="./images/clients/3.svg" alt=""';
  }

  img {
    --attrs: 'src="./images/clients/4.svg" alt=""';
  }

  img {
    --attrs: 'src="./images/clients/5.svg" alt=""';
  }

  img {
    --attrs: 'src="./images/clients/6.svg" alt=""';
  }
}
section#video-block.black {
  header-section[centered] {
    highlighted-text.green {
      --text: 'Our History';
    }
    h2 {
      --text: "Strategy for Every Stage in the Client's Journey";
    }
  }

  play-area {
    background: no-repeat center / contain
      bgImageMultiType('../../images/strategy.png');

    button {
      --attr-type: 'button';
      --attr-title: 'Play the video';
      --attr-data-open-dialog-id: 'history-video';

      svg {
        --attrs: 'width="109" height="109" viewBox="0 0 109 109" fill="none" xmlns="http://www.w3.org/2000/svg"';

        circle {
          --attrs: 'cx="54.5" cy="54.5" r="54.5" fill="white" fill-opacity="0.08"';
        }
        circle {
          --attrs: 'cx="54.5" cy="54.5" r="54.35" stroke="white" stroke-opacity="0.42" stroke-width="0.3"';
        }
        circle {
          --attrs: 'cx="54.5" cy="54.5" r="43.35" fill="white" fill-opacity="0.17" stroke="#D0D0D0" stroke-width="0.3"';
        }
        circle.main-circle {
          --attrs: 'cx="54.5" cy="54.5" r="33.5" fill="white"';
        }
        path {
          --attrs: 'd="M51.0194 62.7189L59.6309 55.4472C59.7696 55.33 59.881 55.1846 59.9574 55.021C60.0337 54.8574 60.0732 54.6795 60.0732 54.4994C60.0732 54.3193 60.0337 54.1414 59.9574 53.9778C59.881 53.8142 59.7696 53.6688 59.6309 53.5516L51.0194 46.2799C50.1974 45.5859 48.9277 46.1613 48.9277 47.2278V61.7731C48.9277 62.8396 50.1974 63.4149 51.0194 62.7189Z" fill="#3B3A5D"';
        }
      }
    }
  }

  dialog#history-video[data-modal-dialog] {
    div {
      span {
        --text: 'Your video could be here';
      }
    }
  }
}
section#contact {
  text-block {
    header-section {
      highlighted-text.yellow {
        --text: 'Proccess';
      }
      h2 {
        --text: 'Contact Us. It’s Easy.';
      }
      p {
        --text: 'Leverage agile frameworks to provide a robust synopsis for high level overviews. Iterative approaches to corporate strategy foster collaborative.';
      }
    }

    aside {
      ul {
        three-section-block[role='listitem'][revert] {
          img {
            --attr-src: './images/icons/yellow_yellow-bg_tel.svg';
            --attrs: "loading='lazy' alt=''";
          }

          strong {
            a {
              --attr-href: 'tel:+88005553535';
              --text: '+1 800 100 900';
            }
          }
          p {
            --text: 'Call Today';
          }
        }
        three-section-block[role='listitem'][revert] {
          img {
            --attr-src: './images/icons/greenClocks.svg';
            --attrs: "loading='lazy' alt=''";
          }

          strong {
            --text: '9AM - 5PM';
          }
          p {
            --text: 'Monday To Friday';
          }
        }
        three-section-block[role='listitem'][revert] {
          img {
            --attr-src: './images/icons/grayLocation.svg';
            --attrs: "loading='lazy' alt=''";
          }

          strong {
            address {
              --text: '195 Devonshire St Boston, MA 02110';
            }
          }
          p {
            --text: 'USA Office';
          }
        }
      }
    }
  }
  images-block {
    x-img {
      --attr-source: './images/contact-us-decor.png';
      --attrs: "loading='lazy' alt='' webp avif";
    }
  }
}
